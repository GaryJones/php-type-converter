{
	"name": "garyjones/php-type-converter",
	"type": "library",
	"description": "Converts one resource to another (XML, JSON, Object, Array, Serialization). PSR-2. Supports nested resources.",
	"keywords": ["type", "converter"],
	"homepage": "https://github.com/GaryJones/php-type-converter",
	"license": "MIT",
	"authors": [
		{
			"name": "Miles Johnson",
			"homepage": "http://milesj.me"
		},
		{
			"name": "Gary Jones",
			"email": "gary@garyjones.co.uk"
		}
	],
	"require": {
		"php": ">=5.3"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*@stable",
		"squizlabs/php_codesniffer": "*"
	},
	"support": {
		"source": "https://github.com/milesj/php-type_converter"
	},
	"autoload": {
		"psr-0": { "GaryJones\\TypeConverter": "src/" }
	},
	"minimum-stability": "dev"
}
